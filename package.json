    "@ngx-translate/http-loader": "^6.0.0",
    "angular-csv-ext": "^1.0.4",
    "angular-svg-icon": "^11.0.0",
    "apexcharts": "^3.23.1",
    "bootstrap": "^4.6.0",
    "browser-detect": "^0.2.28",
    "chart.js": "^2.9.4",
