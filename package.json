    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.3",
    "@angular/language-service": "~10.2.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/googlemaps": "^3.39.12",
    "@types/jasmine": "~3.6.1",
